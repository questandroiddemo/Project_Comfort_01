<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#6A5A5A"
    android:scrollbarSize="20dp"
    android:scrollbarStyle="outsideInset"
    tools:context=".fragment1">

        <ImageButton
            android:id="@+id/button5"
            android:layout_width="60dp"
            android:layout_height="50dp"
            android:layout_marginStart="64dp"
            android:layout_marginTop="88dp"
            android:layout_marginEnd="100dp"
            android:adjustViewBounds="true"
            android:scaleType="centerInside"
            android:src="@android:drawable/arrow_up_float"
            app:layout_constraintEnd_toStartOf="@+id/button1"
            app:layout_constraintHorizontal_bias="0.194"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/button1"
            android:layout_width="220dp"
            android:layout_height="140dp"
            android:layout_marginStart="260dp"
            android:layout_marginTop="100dp"
            android:adjustViewBounds="true"
            android:scaleType="centerInside"
            android:src="@drawable/ac1"
            app:layout_constraintEnd_toStartOf="@+id/button4"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/button2"
            android:layout_width="220dp"
            android:layout_height="140dp"
            android:layout_marginTop="100dp"
            android:layout_marginEnd="140dp"
            android:adjustViewBounds="true"
            android:scaleType="centerInside"
            android:src="@drawable/ac4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/button3"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/button3"
            android:layout_width="220dp"
            android:layout_height="140dp"
            android:layout_marginTop="100dp"
            android:adjustViewBounds="true"
            android:scaleType="centerInside"
            android:src="@drawable/ac3"
            app:layout_constraintEnd_toStartOf="@+id/button2"
            app:layout_constraintStart_toEndOf="@+id/button4"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/button4"
            android:layout_width="220dp"
            android:layout_height="140dp"
            android:layout_marginTop="100dp"
            android:adjustViewBounds="true"
            android:scaleType="centerInside"
            android:src="@drawable/ac2"
            app:layout_constraintEnd_toStartOf="@+id/button3"
            app:layout_constraintStart_toEndOf="@+id/button1"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="SpeakableTextPresentCheck" />

        <SeekBar
            android:id="@+id/slider2"
            android:layout_width="189dp"
            android:layout_height="400dp"
            android:layout_marginEnd="1088dp"
            android:paddingLeft="0dp"
            android:rotation="270"
            android:valueFrom="10"
            android:valueTo="40"
            app:layout_constraintBottom_toTopOf="@+id/button"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button5"
            app:layout_constraintVertical_bias="0.481"
            app:thumbColor="#2196F3"
            app:trackColorActive="#F44336"
            app:trackColorInactive="@color/white"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/button"
            android:layout_width="60dp"
            android:layout_height="50dp"
            android:layout_marginStart="64dp"
            android:layout_marginEnd="1156dp"
            android:layout_marginBottom="344dp"
            android:src="@android:drawable/arrow_down_float"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="SpeakableTextPresentCheck" />

        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="984dp"
            android:layout_height="112dp"
            android:layout_marginStart="36dp"
            android:layout_marginTop="188dp"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.811"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button3">

                <ToggleButton
                    android:id="@+id/toggleButton"
                    android:layout_width="150dp"
                    android:layout_height="110dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginBottom="100dp"
                    android:background="@drawable/selector"
                    android:text="ToggleButton"
                    android:textOff=""
                    android:textOn=""
                    android:adjustViewBounds="true"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/toggleButton2"
                    app:layout_constraintHorizontal_bias="0.45"
                    app:layout_constraintStart_toStartOf="parent"/>

                <ToggleButton
                    android:id="@+id/toggleButton2"
                    android:layout_width="140dp"
                    android:layout_height="100dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="30dp"
                    android:adjustViewBounds="true"
                    android:text="ToggleButton"
                    android:textOff=""
                    android:textOn=""
                    android:background="@drawable/selector1"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent" />

                <Button
                    android:id="@+id/button34"
                    android:layout_width="213dp"
                    android:layout_height="112dp"
                    android:layout_marginRight="20dp"
                    android:layout_weight="1"
                    android:text="Max A/C"
                    android:textSize="30dp" />

                <Button
                    android:id="@+id/button35"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="A/C"
                    android:textSize="30dp" />

                <ToggleButton
                    android:id="@+id/toggleButton3"
                    android:layout_width="150dp"
                    android:layout_height="130dp"
                    android:layout_marginStart="0dp"
                    android:layout_marginBottom="0dp"
                    android:text="ToggleButton"
                    android:textOff=""
                    android:textOn=""
                    android:elevation="20dp"
                    android:background="@drawable/selector2"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.656"
                    app:layout_constraintStart_toStartOf="parent" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="1244dp"
            android:layout_height="149dp"
            android:layout_marginBottom="32dp"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout">

                <Button
                    android:id="@+id/button23"
                    android:layout_width="112dp"
                    android:layout_height="93dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_weight="1"
                    android:text="OFF"
                    android:textSize="30dp" />

                <Button
                    android:id="@+id/btn1"
                    android:layout_width="120dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="1"
                    android:textSize="40dp" />

                <Button
                    android:id="@+id/btn2"
                    android:layout_width="120dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="2"
                    android:textSize="40dp" />

                <Button
                    android:id="@+id/btn3"
                    android:layout_width="120dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="3"
                    android:textSize="40dp" />

                <Button
                    android:id="@+id/btn4"
                    android:layout_width="120dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="4"
                    android:textSize="40dp" />

                <Button
                    android:id="@+id/btn5"
                    android:layout_width="120dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="5"
                    android:textSize="40dp" />

                <Button
                    android:id="@+id/btn6"
                    android:layout_width="120dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="6"
                    android:textSize="40dp" />

                <Button
                    android:id="@+id/btn7"
                    android:layout_width="120dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="7"
                    android:textSize="40dp" />

                <Button
                    android:id="@+id/button31"
                    android:layout_width="wrap_content"
                    android:layout_height="86dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_weight="1"
                    android:text="Auto"
                    android:textSize="30dp" />
        </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>